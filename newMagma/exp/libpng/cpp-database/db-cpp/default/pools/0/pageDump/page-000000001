   <https://www.gnu.org/licenses/>.  */png_charpppdouble **png_doublepppng_fixed_point **int **png_fixed_point_pppng_charpppng_const_charpppng_int_16 *png_int_16 **short **png_int_16pppng_uint_16 **png_uint_16pppng_int_32 **png_int_32pppng_uint_32 **unsigned int **png_uint_32pppng_const_doubleppng_doublepconst size_tconst size_t *png_const_size_tppng_size_tpconst png_fixed_pointconst png_fixed_point *png_const_fixed_point_ppng_fixed_point_pconst png_int_16const png_int_16 *png_const_int_16ppng_int_16pconst png_uint_16 *png_uint_16pconst png_int_32const png_int_32 *png_const_int_32ppng_int_32ppng_const_uint_32ppng_uint_32pconst png_byte *png_const_voidppng_ptrdiff_tpng_size_tpng_int_16PNG_FIXED_EXPORT(ordinal,type,name,args)PNG_EXPORT(ordinal, type, name, args);PNG_FP_EXPORT(ordinal,type,name,args)PNG_RESTRICTPNG_USE_RESULT__attribute__((__warn_unused_result__))PNG_PEDANTIC_WARNINGS_SUPPORTEDPNG_CALLBACK(type,name,args)type (PNGCBAPI name) PNGARG(args)PNG_REMOVED(ordinal,type,name,args,attributes)PNG_EXPORT(ordinal,type,name,args)PNG_EXPORTA(ordinal, type, name, args, PNG_EMPTY)PNG_EXPORTA(ordinal,type,name,args,attributes)PNG_FUNCTION(PNG_EXPORT_TYPE(type), (PNGAPI name), PNGARG(args), PNG_LINKAGE_API attributes)PNG_EXPORT_TYPE(type)PNG_IMPEXP typePNG_FUNCTION(type,name,args,attributes)attributes type name argsPNGCAPIPNGCBAPIPNGARG(arglist)arglistPNG_CONSTPNGCONF_HPNG_BUILDING_SYMBOL_TABLE!defined(PNG_NO_USE_READ_MACROS) && !defined(PNG_USE_READ_MACROS)PNGARGdefined(_Windows) || defined(_WINDOWS) || defined(WIN32) ||\PNG_API_RULE == 2defined(__GNUC__) || (defined(_MSC_VER) && (_MSC_VER >= 800))PNG_API_RULE == 1PNG_API_RULE == 1 && !defined(PNGAPI)defined(PNGAPI) && !defined(PNG_USER_PRIVATEBUILD)(defined(_MSC_VER) && _MSC_VER < 800) ||\PNG_EXPORT_TYPEPNG_DLL_IMPORT(defined(__IBMC__) || defined(__IBMCPP__)) && defined(__OS2__)defined(PNG_USE_DLL) && defined(PNG_DLL_IMPORT)PNG_FUNCTIONPNG_EXPORTAPNG_REMOVEDPNG_CALLBACKPNG_NO_PEDANTIC_WARNINGSdefined(__clang__) && defi